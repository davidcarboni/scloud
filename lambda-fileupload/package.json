    "@types/chai": "*",
    "@types/mocha": "*",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "chai": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-import-resolver-typescript": "*",
    "eslint-plugin-import": "*",
    "mocha": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "*",
    "@aws-sdk/s3-presigned-post": "*",
    "@aws-sdk/s3-request-presigner": "*"
