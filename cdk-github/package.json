{
  "name": "@scloud/cdk-github",
  "version": "0.2.0",
  "description": "Automates setting variables and secrets on a Github repo/environment based on output produced by the ghaUser() feature of @scloud/cdk-patterns",
  "main": "dist/github.js",
  "bin": {
    "cdk-github": "dist/github.js"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "lint": "eslint --fix src",
    "compile": "tsc",
    "package": "mkdir -p dist && cp -r js/* dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidcarboni/scloud.git"
  },
  "keywords": [
    "AWS",
    "CDK",
    "Github",
    "Actions",
    "secrets",
    "variables"
  ],
  "author": "David Carboni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidcarboni/scloud/issues"
  },
  "homepage": "https://github.com/davidcarboni/scloud#readme",
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@types/libsodium-wrappers": "*",
    "@types/node": "*",
    "eslint": "*",
    "source-map-support": "*",
    "ts-node": "*",
    "typescript": "*",
    "typescript-eslint": "^7.8.0"
  },
  "dependencies": {
    "@octokit/rest": "*",
    "libsodium-wrappers": "*"
  }
}