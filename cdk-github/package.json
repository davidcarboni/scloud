{
  "name": "@scloud/cdk-github",
  "version": "0.1.2",
  "description": "Automates setting variables and secrets on a Github repo/environment based on output produced by the ghaUser() feature of @scloud/cdk-patterns",
  "main": "dist/index.js",
  "bin": {
    "cdk-github": "dist/update.sh"
  },
  "scripts": {
    "lint": "eslint --fix --ext ts",
    "package": "tsc && cp update.sh dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidcarboni/scloud.git"
  },
  "keywords": [
    "AWS",
    "CDK",
    "Github",
    "Actions",
    "secrets",
    "variables"
  ],
  "author": "David Carboni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidcarboni/scloud/issues"
  },
  "homepage": "https://github.com/davidcarboni/scloud#readme",
  "devDependencies": {
    "@types/libsodium-wrappers": "*",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-import-resolver-typescript": "*",
    "eslint-plugin-import": "*",
    "source-map-support": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "dependencies": {
    "@octokit/rest": "*",
    "libsodium-wrappers": "*"
  }
